{
  "name": "mkrc-ts",
  "version": "1.0.0",
  "description": "CLI for generating React component files in module folders",
  "keywords": [
    "react",
    "component",
    "generator",
    "cli"
  ],
  "author": "Swagatam Roy",
  "license": "MIT",
  "homepage": "https://github.com/theonlysroy/mkrc-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theonlysroy/mkrc-ts.git"
  },
  "bugs": {
    "url": "https://github.com/theonlysroy/mkrc-ts/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "mknrc": "./bin/index.js"
  },
  "scripts": {
    "start": "node ./bin/index.js",
    "test": "node --test",
    "prepublishOnly": "npm test && npm pack --dry-run"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "fs-extra": "^11.3.3",
    "ora": "^9.3.0"
  }
}